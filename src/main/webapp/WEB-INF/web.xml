<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns="http://xmlns.jcp.org/xml/ns/javaee" xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_3_1.xsd" id="WebApp_ID" version="3.1">
	<display-name>Assignment3</display-name>
	<welcome-file-list>
		<welcome-file>index.html</welcome-file>
		
	</welcome-file-list>
	<servlet>
		<servlet-name>login</servlet-name>
		<servlet-class>com.auca.service.Signup</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>login</servlet-name>
		<url-pattern>/login</url-pattern>
	</servlet-mapping>
	<servlet>
		<servlet-name>Submit</servlet-name>
		<servlet-class>com.auca.service.Submission</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>Submit</servlet-name>
		<url-pattern>/submit</url-pattern>
	</servlet-mapping>
	
	<filter>
		<filter-name>filtered</filter-name>
		<filter-class>com.auca.service.Filtered</filter-class>
	</filter>
	<filter-mapping>
		<filter-name>filtered</filter-name>
		<url-pattern>/login</url-pattern>
	</filter-mapping>
</web-app>